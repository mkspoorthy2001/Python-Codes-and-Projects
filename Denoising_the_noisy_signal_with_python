      },
      "source": [
        "import numpy as np                                     #find the mean of the filter\r\n",
        "import matplotlib.pyplot as plt                        #plottiing\r\n",
        "import copy                                            #to keep the copy of the noisy signal"
      ],
      "execution_count": 1,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 279
        },
        "id": "SpNeKmBv1FqJ",
        "outputId": "6e2c7446-12da-4ce9-909f-07afbc81d0a7"
      },
      "source": [
        "N = 10001                               #time points\r\n",
        "time = np.linspace(0,4*np.pi,N)         # time vector\r\n",
        "\r\n",
        "signal = np.zeros(N)                    #initially the value of the signal is 0\r\n",
        "\r\n",
        "for j in range(1,4):                    #creating our signal using for loop\r\n",
        "  signal += np.cos(j*time)**j          # signal = cos(j*time)**j where j=1,2,3\r\n",
        "\r\n",
        "noisysignal = signal + np.random.randn(N)           #adding gaussian distributed random noise to original noiseless signal \r\n",
        "plt.plot(time,noisysignal,time,signal)             \r\n",
        "\r\n",
        "\r\n",
        "#plt.plot(time,signal)                #original noiseless signal\r\n",
        "plt.xlabel(\"time(0,4pi)\")\r\n",
        "plt.ylabel(\"Signal\")\r\n",
        "plt.show()"
      ],
      "execution_count": 2,
      "outputs": [
