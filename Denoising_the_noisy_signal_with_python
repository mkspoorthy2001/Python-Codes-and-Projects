      "source": [
        "def meansmooth(signalIn,k):            #defining the function to find 1)mean  2)filter the signal  3)correlation coefficient\r\n",
        "  filtsignal = copy.deepcopy(noisysignal)\r\n",
        "  for t in range(N):\r\n",
        "    filtsignal[t] = np.mean( noisysignal[ np.max((0,t-k)):np.min((N,t+k)) ])\r\n",
        "  return filtsignal\r\n",
        "\r\n",
        "kvals = np.arange(5,41)   #k value varies from 5 to 40\r\n",
        "signalCorrs = []\r\n",
        "for ki in kvals:\r\n",
        "  #filter the signal\r\n",
        "  fsig = meansmooth(noisysignal,ki)   #calling the meansmooth function \r\n",
        "\r\n",
        "  #compute the correlation signal and original\r\n",
        "  signalCorrs.append(np.corrcoef(fsig,signal)[0,1])    #finding correlation coefficient using corrcoef() funtion of numpy"
      ],
      "execution_count": 5,
      "outputs": []
    },
