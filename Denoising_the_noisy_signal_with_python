      },
      "source": [
        "#running-mean filter\r\n",
        "\r\n",
        "\r\n",
        "filtsignal = copy.deepcopy(noisysignal)      #filtsignal having the copy of noisysignal\r\n",
        "\r\n",
        "k = 15                              #mean smoothing window\r\n",
        "\r\n",
        "for t in range(N):                  #looping over N for each time point t in the filtered signal\r\n",
        "  lowbnd = np.max((0,t-k))          # 0 if t-k is negative or else t-k\r\n",
        "  uppbnd = np.min((N,t+k))          # N if t+k is out of boundary or else t+k\r\n",
        "  filtsignal[t] = np.mean( noisysignal[ lowbnd:uppbnd ])   #finding out the mean for every N step\r\n",
        "\r\n",
        "plt.plot(time,filtsignal,time,signal)\r\n",
        "\r\n",
        "plt.show()"
      ],
      "execution_count": 3,
